{
  "version": 2,
  "name": "thinkphp-project",
  "functions": {
    "public/index.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "builds": [
    {
      "src": "public/index.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "public/index.php"
    }
  ]
}